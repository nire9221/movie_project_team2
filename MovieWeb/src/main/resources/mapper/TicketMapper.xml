<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.map.prac.mapper.MapMapper" >

    <select id="searchTicketAll" resultType="package com.team2.movie.dao.dto.Ticket;">
        SELECT * FROM cgv.Ticket;
    </select>
    
    <insert id="createTicket" parameterType="post">
    INSERT INTO Ticket (groupNum, kakaoId, seatid, ticketPrice, ticketDiscount) 
    VALUES (#{groupNum}, #{kakaoId}, #{seatid}, #{ticketPrice}, #{ticketDiscount})
    </insert>
    
    
</mapper>